---
title: <%= title %>
---

<div class="container-fluid">
<div class="row">

<script src="../javascripts/pgn4web.js" type="text/javascript"></script>
<script src="../fonts/chess-informant-NAG-symbols.js" type="text/javascript"></script>

<script type="text/javascript">
    "use strict";
    SetPgnUrl("<%= game.pgn %>");
    SetImagePath("../images/pgn4web/alpha/48");
    SetImageType("png");
    SetShortcutKeysEnabled(true);
    SetHighlightOption(false);
    SetCommentsIntoMoveText(true);
    SetCommentsOnSeparateLines(true);
    SetGameSelectorOptions("Select game...", false, 0, 0, 0, 15, 15, 0, 0);
</script>

<div class="col-sm-4" style="width: 400px;position:fixed;background-color: white;z-index: 3;">
    <div id="GameSelector"></div>
    <p></p>
    <div id="GameBoard"></div>
    <p></p>
    <div id="GameButtons"></div>
</div>

<div class="visible-xs col-sm-8 col-sm-offset-1" style="padding-top:440px;overflow:scroll;z-index: 2;">
    <b><span class="GameWhite"></span>&nbsp;-&nbsp;<span class="GameBlack"></span>&nbsp;&nbsp;<span class="GameResult"></span></b>
    <div class="GameText"></div>
</div>
<div class="hidden-xs col-sm-8 col-sm-offset-6">
    <b><span class="GameWhite"></span>&nbsp;-&nbsp;<span class="GameBlack"></span>&nbsp;&nbsp;<span class="GameResult"></span></b>
    <div class="GameText"></div>
</div>


</div>
</div>